#include <iostream>
#include <cstdio>
#include <cstring>
#include <algorithm>
#include <vector>
#include <queue>
#include <set>
#include <map>
#include <string>
#include <cmath>
#include <cstdlib>
#include <ctime>
using namespace std;

typedef long long ll;
const int N = 1e4+10;
int md;
int n;
ll x;

int main()
{
    //freopen("in.txt","r",stdin);
    //freopen("out.txt","w",stdout);
    x=1;
	cin>>n>>md;
	for(int i=1;i<=n;i++)
		x=x*i%md;
	cout<<x<<endl;
    return 0;
}
